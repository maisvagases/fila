// This is your Prisma schema file
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model JobPost {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  url          String
  startTime    DateTime
  finishedTime DateTime
  title        String?

  @@map("send_logs")
}